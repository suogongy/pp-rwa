# 环境变量配置文件示例
# 复制此文件为 .env 并填入实际值

# 部署者私钥（不要在生产环境中使用真实私钥）
PRIVATE_KEY=0x1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef

# Infura API Key（用于连接以太坊网络）
INFURA_API_KEY=your_infura_api_key_here

# Etherscan API Key（用于合约验证）
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# 网络配置
LOCAL_RPC_URL=http://127.0.0.1:8545
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/your_infura_api_key_here
MAINNET_RPC_URL=https://mainnet.infura.io/v3/your_infura_api_key_here

# 合约地址（部署后会自动生成，或手动填入已部署的合约地址）
RWA20_ADDRESS=0x1234567890123456789012345678901234567890
RWA721_ADDRESS=0x1234567890123456789012345678901234567890
RWA_STAKING_ADDRESS=0x1234567890123456789012345678901234567890

# 合约参数
RWA20_NAME=Real World Asset Token
RWA20_SYMBOL=RWA
RWA721_NAME=Real World Asset NFT
RWA721_SYMBOL=RWA721
BASE_URI=https://api.example.com/metadata/